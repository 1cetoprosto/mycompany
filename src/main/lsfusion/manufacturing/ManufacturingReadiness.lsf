MODULE ManufacturingReadiness;

REQUIRE Manufacturing;

NAMESPACE Manufacturing;

CLASS ManufacturingReadiness 'Начать производство' {
    first 'Доступны компоненты для первой рперации',
    all 'Доступны компоненты для всех операций'
}

name 'Название' (ManufacturingReadiness m) = staticCaption(m) IF m IS ManufacturingReadiness;

