MODULE PurchasePricelistItem;

REQUIRE PurchasePricelist;

NAMESPACE Purchase;

EXTEND FORM item 
    OBJECTS prlp = PricelistLine 
    PROPERTIES(prlp) READONLY number, startDateTime, endDateTime, note, nameItemVendor, codeItemVendor, unitPrice
    FILTERS item(prlp) = i
;

DESIGN item {
    purchase {
        MOVE BOX(prlp);
    }
}
