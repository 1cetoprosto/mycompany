MODULE PurchasePricelistPartner;

REQUIRE PurchasePricelist;

NAMESPACE Purchase;

EXTEND FORM partner
    OBJECTS prp = Pricelist 
    PROPERTIES(prp) series, number, startDate, endDate, note, NEW, DELETE 
    FILTERS vendor(prp) = p
;

DESIGN partner {
    vendor {
        MOVE BOX(prp) { fill = 1; };
    }
}
