MODULE InvoicingSettings;

REQUIRE Numerator, Invoicing;

NAMESPACE Invoicing;

numerator 'Нумератор' = DATA Numerator();
nameNumerator 'Нумератор' = name(numerator());

FORM invoicingSettings 'Настройки'
  PROPERTIES nameNumerator()
;

NAVIGATOR {
    invoicing {
        settings{
            NEW invoicingSettings;        
        }
    }
}