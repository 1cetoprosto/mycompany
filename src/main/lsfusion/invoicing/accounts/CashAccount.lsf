MODULE CashAccount;

REQUIRE Invoicing, Account;

NAMESPACE Invoicing;

CLASS CashAccount 'Касса' : Account;

newCashAccount 'Касса' () {
    NEWSESSION {
        NEW c = CashAccount {
            
            SHOW account OBJECTS a = c;  
        }
    }
} IMAGE 'add.png';

EXTEND FORM accounts
    PROPERTIES newCashAccount() DRAW a TOOLBAR
    
    LIST CashAccount OBJECT a
;
